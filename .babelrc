{
    "presets": [
        [ "stage-1"],
        ["es2015", {"modules": false}],
        ["@babel/preset-typescript",  {
            "modules": false,
            "targets": {
              "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            },
            "useBuiltIns": "usage"
          }]
    ],
    "plugins": ["transform-runtime", "@babel/plugin-transform-object-assign"]
}
